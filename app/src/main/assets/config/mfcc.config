processArrayFields = 1
includeSingleElementFields = 0
preserveFieldNames = 1
inverse = 0
magnitude = 1
phase = 0
joinMagphase = 0
normalise = 0
power = 0
dBpsd = 0
dBpnorm = 90.302000
mindBp = -102
 

  ;;;; default (template) configuration section for component 'cMelspec' ;;;;
[melspec:cMelspec]
reader.dmLevel = fftmag
writer.dmLevel = melspec
// nameAppend = 
copyInputName = 1
