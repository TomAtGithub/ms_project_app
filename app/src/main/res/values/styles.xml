<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="headline" parent="TextAppearance.Material3.HeadlineLarge">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">10dp</item>
    </style>
    <style name="title" parent="TextAppearance.Material3.HeadlineMedium">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">10dp</item>
    </style>
    <style name="text" parent="TextAppearance.Material3.BodyLarge">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">10dp</item>
    </style>
    <style name="button" parent="ShapeAppearanceOverlay.Material3.Button">
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:layout_width">150dp</item>
    </style>
    <style name="margins" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">10dp</item>
    </style>
</resources>